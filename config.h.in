/* config.h.in - Configuration template for libcaca */

/* Package information */
#define PACKAGE_NAME "@PACKAGE_NAME@"
#define PACKAGE_VERSION "@PACKAGE_VERSION@"
#define PACKAGE_STRING "@PACKAGE_STRING@"
#define PACKAGE_BUGREPORT "@PACKAGE_BUGREPORT@"
#define PACKAGE_URL "@PACKAGE_URL@"
#define PACKAGE_TARNAME "@PACKAGE_TARNAME@"

/* Define to 1 if you have the ANSI C header files. */
#cmakedefine STDC_HEADERS 1

/* Standard C library headers */
#cmakedefine HAVE_STDIO_H 1
#cmakedefine HAVE_STDLIB_H 1
#cmakedefine HAVE_STRING_H 1
#cmakedefine HAVE_STRINGS_H 1
#cmakedefine HAVE_MEMORY_H 1
#cmakedefine HAVE_STDARG_H 1
#cmakedefine HAVE_ERRNO_H 1
#cmakedefine HAVE_UNISTD_H 1
#cmakedefine HAVE_SIGNAL_H 1
#cmakedefine HAVE_SYS_TYPES_H 1
#cmakedefine HAVE_SYS_STAT_H 1
#cmakedefine HAVE_SYS_TIME_H 1
#cmakedefine HAVE_SYS_IOCTL_H 1
#cmakedefine HAVE_TERMIOS_H 1
#cmakedefine HAVE_INTTYPES_H 1
#cmakedefine HAVE_STDINT_H 1
#cmakedefine HAVE_LOCALE_H 1
#cmakedefine HAVE_GETOPT_H 1
#cmakedefine HAVE_DLFCN_H 1
#cmakedefine HAVE_ENDIAN_H 1
#cmakedefine HAVE_ARPA_INET_H 1
#cmakedefine HAVE_NETINET_IN_H 1
#cmakedefine HAVE_WINDOWS_H 1
#cmakedefine HAVE_WINSOCK2_H 1
#cmakedefine HAVE_CONIO_H 1
#cmakedefine HAVE_ZLIB_H 1

/* Functions */
#cmakedefine HAVE_GETENV 1
#cmakedefine HAVE_PUTENV 1
#cmakedefine HAVE_ATEXIT 1
#cmakedefine HAVE_SIGNAL 1
#cmakedefine HAVE_GETOPT_LONG 1
#cmakedefine HAVE_GETTIMEOFDAY 1
#cmakedefine HAVE_USLEEP 1
#cmakedefine HAVE_SLEEP 1
#cmakedefine HAVE_SNPRINTF 1
#cmakedefine HAVE_VSNPRINTF 1
#cmakedefine HAVE_SPRINTF_S 1
#cmakedefine HAVE_VSNPRINTF_S 1
#cmakedefine HAVE_STRCASECMP 1
#cmakedefine HAVE_IOCTL 1
#cmakedefine HAVE_HTONS 1

/* Math functions */
#cmakedefine HAVE_FSIN_FCOS 1
#cmakedefine HAVE_FLDLN2 1

/* Graphics libraries */
#cmakedefine HAVE_CURSES_H 1
#cmakedefine HAVE_NCURSES_H 1
#cmakedefine HAVE_NCURSES_NCURSES_H 1
#cmakedefine HAVE_NCURSESW_NCURSES_H 1
#cmakedefine HAVE_RESIZETERM 1
#cmakedefine HAVE_RESIZE_TERM 1
#cmakedefine HAVE_GL_GL_H 1
#cmakedefine HAVE_GL_GLUT_H 1
#cmakedefine HAVE_OPENGL_GL_H 1
#cmakedefine HAVE_GLUTCHECKLOOP 1
#cmakedefine HAVE_GLUTCLOSEFUNC 1
#cmakedefine HAVE_IMLIB2_H 1
#cmakedefine HAVE_X11_XKBLIB_H 1

/* S-Lang support */
#cmakedefine HAVE_SLANG_H 1
#cmakedefine HAVE_SLANG_SLANG_H 1
#cmakedefine HAVE_SLSMG_UTF8_ENABLE 1

/* Windows-specific */
#cmakedefine HAVE_WINDOWS_H 1
#cmakedefine HAVE_WINSOCK2_H 1
#cmakedefine HAVE_CONIO_H 1
#cmakedefine HAVE_SYS_SOCKET_H 1
#cmakedefine ALLOCCONSOLE_IN_WINDOWS_H 1
#cmakedefine SCREENUPDATE_IN_PC_H 1

/* Platform-specific features */
#cmakedefine USE_WIN32 1
#cmakedefine USE_CONIO 1
#cmakedefine USE_COCOA 1
#cmakedefine USE_GL 1
#cmakedefine USE_IMLIB2 1
#cmakedefine USE_KERNEL 1
#cmakedefine USE_NCURSES 1
#cmakedefine USE_PLUGINS 1
#cmakedefine USE_SLANG 1
#cmakedefine USE_VGA 1
#cmakedefine USE_X11 1
#cmakedefine USE_NULL 1

/* Debugging and profiling */
#cmakedefine DEBUG 1
#cmakedefine PROF 1

/* Define to empty if `const' does not conform to ANSI C. */
/* #undef const */

/* Define to `__inline__' or `__inline' if that's what the C compiler
   calls it, or to nothing if 'inline' is not supported under any name.  */
#ifndef __cplusplus
#if defined(_MSC_VER) && !defined(inline)
#define inline __inline
#endif
/* #undef inline */
#endif

/* Windows-specific compatibility */
#if defined(_WIN32) && defined(_MSC_VER)
#if _MSC_VER < 1900
#define snprintf _snprintf
#define vsnprintf _vsnprintf
#endif
#define strcasecmp _stricmp
#define strncasecmp _strnicmp
#endif
